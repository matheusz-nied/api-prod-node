    "@types/nock": "^11.1.0",
    "@types/node": "^20.4.1",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "jest": "^29.6.1",
    "nock": "^13.3.3",
    "pino-pretty": "^10.2.0",
